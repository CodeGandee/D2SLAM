# D2SLAM JetPack 6.2 Environment Configuration
# Copy this file to .env and modify values as needed

# SSH Configuration
SSH_PASSWORD=d2slam123
SSH_PORT=2222

# Build Configuration
CERES_VERSION=2.1.0
CMAKE_VERSION=3.24.1
OPENCV_VERSION=4.10.0
ROS_VERSION=noetic
FAISS_VERSION=1.7.4
USE_PROC=8

# CUDA Configuration for Jetson Orin (8.7) and Orin NX (8.7)
CUDA_ARCH_BIN=8.7
ENABLE_NEON=ON

# NVIDIA Runtime Configuration
NVIDIA_VISIBLE_DEVICES=all
NVIDIA_DRIVER_CAPABILITIES=all

# Container Configuration
CONTAINER_TIMEZONE=Asia/Beijing

# D2SLAM Repository Configuration
D2SLAM_REPO=https://github.com/CodeGandee/D2SLAM.git
D2SLAM_BRANCH=main
